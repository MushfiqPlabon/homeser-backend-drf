{
  "version": 2,
  "builds": [
    {
      "src": "homeser/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.12"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "homeser/wsgi.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "homeser.settings"
  },
  "functions": {
    "homeser/wsgi.py": {
      "excludeFiles": "**/node_modules/**,**/tests/**,**/__pycache__/**,**/.git/**,.env,.env.example,**/.venv/**,**/venv/**,**/requirements*.txt,uv.lock,*.md,*.toml,**/migrations/**"
    }
  },
  "github": {
    "enabled": false
  }
}