{
  "version": 2,
  "builds": [
    {
      "src": "homeser/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.12"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "homeser/wsgi.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "homeser.settings"
  },
  "functions": {
    "homeser/wsgi.py": {
      "excludeFiles": "{.env.example,.env,.venv,venv/,.git/**}"
    }
  }
}